{
  "name": "tanabata-blog",
  "version": "1.0.0",
  "description": "amanojs's blog.",
  "main": "./server/index.js",
  "scripts": {
    "client": "webpack-dev-server --config ./client/config/webpack.config.dev.js --open --mode development",
    "server": "nodemon --ignore blogs/ server/server.js --exec babel-node",
    "build": "webpack --config ./client/config/webpack.config.js --mode development",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "start": "npm run build && npm run server",
    "md": "processmd blogs/markdowns/*.md --stdout --preview 100 --outputDir blogs/jsons > blogs/summary.json"
  },
  "author": "amanojs",
  "license": "ISC",
  "babel": {
    "presets": [
      "babel-preset-env",
      "babel-preset-react",
      "babel-preset-es2015",
      "babel-preset-stage-0"
    ],
    "plugins": [
      "styled-jsx/babel"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-responsive": "^8.0.2",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/styled-jsx": "^2.2.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^5.2.0",
    "html-webpack-plugin": "^4.3.0",
    "nodemon": "^2.0.4",
    "processmd": "^4.5.0",
    "ts-loader": "^8.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^12.2.1",
    "styled-jsx": "^3.3.0",
    "typescript": "^3.9.7"
  }
}
